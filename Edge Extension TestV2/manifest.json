{
    "name": "Test2",
    "version": "0.0.0.1",
    "manifest_version": 3,
    "description": "An extension that adds a shortcut to search for the currently selected text",
	"permissions": ["activeTab", "cookies", "storage","tabs","browsingData","scripting", "commands", "contextMenus", "tabs"],
	"icons": {
        "16": "icons/cse16x16.png"
    },
	"content_scripts": [
        {
            "matches": [
              "<all_urls>"
            ],
            "js": ["content-scripts/content.js"]
        }
    ],
	"background": {
		"service_worker": "background.js"
	  },
	"commands": {
    "open-new-tab": {
      "suggested_key": {
        "default": "Ctrl+Shift+E",
        "mac": "Command+Shift+E"
      },
      "description": "Open a new tab"
    }
  }
}